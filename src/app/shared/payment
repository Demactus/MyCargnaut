

export class Payment {
    id: string;
    price: number;
    paymentStatus: boolean;



    constructor(id: string, price: number, paymentStatus: boolean) {
        this.id = id;
        this.price = price;
        this.paymentStatus = paymentStatus;
    }


    pay(price: number){
        if (price === this.price){
            this.paymentStatus = true;
        }
    }
}
